{
  "name": "easy-front-vue-cli3",
  "version": "1.0.0",
  "private": true,
  "description": "easy-front-vue-cli3",
  "scripts": {
    "build": "vue-cli-service build --mode production",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "commit": "git cz",
    "dev": "vue-cli-service serve",
    "new:comp": "node ./scripts/component-generator",
    "new:view": "node ./scripts/view-generator",
    "pre-release": "vue-cli-service build --mode pre-release",
    "test": "vue-cli-service build --mode test",
    "genlog": "conventional-changelog -p angular -i .github/CHANGELOG.md -s"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "crypto-js": "^4.0.0",
    "vant": "^2.12.12",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1"
  },
  "devDependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-e2e-cypress": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "~4.3.1",
    "@vue/cli-plugin-vuex": "~4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "chalk": "^4.0.0",
    "commitizen": "^4.0.4",
    "compression-webpack-plugin": "^3.1.0",
    "cz-customizable": "^6.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "image-webpack-loader": "^6.0.0",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "img-loader": "^3.0.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "postcss-pxtorem": "^5.1.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "svg-sprite-loader": "^4.2.5",
    "terser-webpack-plugin": "^4.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
